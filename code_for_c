int n = 5;
		
		for(int i = 1; i<=n; i++) {
			System.out.print("* ");
		}
		System.out.println();
		
		for(int i = 2; i<=n-1; i++) {
			System.out.print("*");
			for(int j= 1; j<=n-1; j++) {
				System.out.print(" ");
			}
			System.out.println();
		}
		for(int i = 1; i<=n; i++) {
			System.out.print("* ");
		}
		System.out.println();
